{
    "name": "duyler/action-bus",
    "type": "library",
    "description": "Duyler action bus",
    "license": "MIT",
    "keywords": [
        "action-bus"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/docs/action-bus/"
    },
    "require": {
        "php": "^8.3",
        "duyler/dependency-injection": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30",
        "squizlabs/php_codesniffer": "^4.0",
        "vimeo/psalm": "^5.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Duyler\\ActionBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\ActionBus\\Test\\": ["tests/"]
        }
    }
}
